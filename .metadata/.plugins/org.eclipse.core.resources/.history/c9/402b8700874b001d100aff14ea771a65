package com.di.anno;

public class Danger {
	
	private String danger;

	public String getDanger() {
		return danger;
	}

	public void setDanger(String danger) {
		this.danger = danger;
	}

	public Danger() {
		super();
		System.out.println("these are very dangerous hoooooh.......");
	}

	public Danger(String danger) {
		super();
		this.danger = danger;
	}
	
	

}


