package com.di.anno;

import org.springframework.stereotype.Component;

@Component
public class Pet {
	private String pets;
	
	public Pet()
	{
		super();
		System.out.println("these pets are likely Dog, Cat, Fish....");
	}

	public String getPets() {
		return pets;
	}

	public void setPets(String pets) {
		this.pets = pets;
	}

	@Override
	public String toString() {
		return "Pet [pets=" + pets + "]";
	}
	
	

}
